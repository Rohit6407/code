import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner s= new Scanner(System.in);
        System.out.println("enter the marks of maths : ");
        float m=s.nextFloat();
        System.out.println("enter the marks of science: ");
        float sc=s.nextFloat();
        System.out.println("enter the marks of hindi: ");
        float h=s.nextFloat();
        System.out.println("enter the marks of socalscience: ");
        float se=s.nextFloat();
        System.out.println("enter the marks of english: ");
        float e=s.nextFloat();
        float sum = m+sc+h+se+e;
        float percantage= (sum/500)*100;
        System.out.println("percantage of student is :");
        System.out.println(percantage);
        
        
    }
}
